<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Societe\BackBundle\Entity\Societe" table="societe">
        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="nom" type="string" column="nom" length="250" nullable="false"/>
        <field name="ville" type="string" column="ville" length="250" nullable="false"/>
        <field name="codePostal" type="string" column="code_postal" length="250" nullable="false"/>
        <field name="telephone" type="string" column="telephone" length="15" nullable="false"/>
        <field name="logo" type="string" column="logo" length="250" nullable="false"/>
    </entity>
</doctrine-mapping>
